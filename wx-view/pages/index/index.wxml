<wxs module="computed" src="./index.wxs" />
<view class="page">
	<image class="log" src="/images/log.svg" mode="heightFix" />
	<view class='circle-container'>
		<view
		 wx:for="{{[1,2,3,4]}}"
		 class="circle"
		 wx:key="index"
		 style="bottom:{{computed.offset(index)}}px; width: {{computed.circleSize(index)}}px; height: {{computed.circleSize(index)}}px;"
		/>

	</view>
</view>

