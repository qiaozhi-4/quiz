<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quiz.mapper.PathMapper">

    <select id="getPathDtoList" resultType="com.quiz.dto.PathDto">
        select *
        from t_path
                 left join m_permission on t_path.permission_id = m_permission.permission_id
    </select>

</mapper>
